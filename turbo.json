{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "staticfiles/**",
        "mediafiles/**"
      ]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "migrate": {
      "cache": false,
      "persistent": false
    },
    "makemigrations": {
      "cache": false,
      "persistent": false
    },
    "collectstatic": {
      "dependsOn": ["^build"],
      "outputs": ["staticfiles/**"]
    },
    "test": {
      "cache": false,
      "persistent": false
    }
  }
}
